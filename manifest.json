{
    "manifest_version": 3,
    "name": "Chrome Extension",
    "version": "1.0",
    "description": "A simple Chrome extension.",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
      "16": "/images/picker2.png",
      "32": "/images/picker1.png",
      "48": "/images/picker3.png"
    }
  }
  